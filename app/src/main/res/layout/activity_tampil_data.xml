<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TampilData">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="412dp"
        android:layout_height="299dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/imageView"
            android:layout_width="400dp"
            android:layout_height="300dp"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/icons" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="334dp"
        android:layout_height="380dp"
        android:layout_marginTop="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/datanama"
            android:layout_width="300dp"
            android:layout_height="30dp"
            android:layout_marginEnd="16dp"
            android:fontFamily="sans-serif-light"
            android:gravity="center_horizontal"
            android:text="Nama "
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            tools:layout_editor_absoluteY="5dp" />

        <TextView
            android:id="@+id/Nim"
            android:layout_width="51dp"
            android:layout_height="26dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-light"
            android:text="NIM"
            android:textColor="#000000"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="46dp" />

        <TextView
            android:id="@+id/datanim"
            android:layout_width="254dp"
            android:layout_height="0dp"
            android:fontFamily="sans-serif-light"
            android:text="datanim"
            android:textColor="#000000"
            android:textSize="15dp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="82dp" />

        <TextView
            android:id="@+id/datatgl"
            android:layout_width="254dp"
            android:layout_height="19dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-light"
            android:text="dataTgl"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="164dp" />

        <TextView
            android:id="@+id/datakelamin"
            android:layout_width="254dp"
            android:layout_height="19dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-light"
            android:text="dataKelamin"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="242dp" />

        <TextView
            android:id="@+id/datajurusan"
            android:layout_width="254dp"
            android:layout_height="19dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-light"
            android:text="datajurusan"
            android:textColor="#000000"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="318dp" />

        <TextView
            android:id="@+id/TanggalLahir"
            android:layout_width="119dp"
            android:layout_height="23dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-light"
            android:text="Tanggal Lahir"
            android:textColor="#000000"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="134dp" />

        <TextView
            android:id="@+id/kelamin"
            android:layout_width="119dp"
            android:layout_height="23dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-light"
            android:text="Jenis Kelamin"
            android:textColor="#000000"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="212dp" />

        <TextView
            android:id="@+id/jurusan"
            android:layout_width="119dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:fontFamily="sans-serif-light"
            android:text="Jurusan"
            android:textColor="#000000"
            android:textSize="18sp"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="287dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>